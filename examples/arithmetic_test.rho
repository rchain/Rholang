// -25, false, true, -0.4

new arithmetic_test in {
    contract arithmetic_test(rtn) = {
        { (5+(-5))*5/1+6+9-40 } |
        { true * true * false } |
        { true + (false + false) } |
        { (5.0/2.5)-2.4 } // This may not be exactly -0.4 due to the nature of floating point arithmetic
    } |
    new rtn in {
        arithmetic_test!(rtn)
    }
}

